{
  "name": "@jirayusueb/genv",
  "version": "0.1.3-rc.0",
  "description": "Environment variable manager for monorepos with shared variable support",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.mts"
    }
  },
  "bin": {
    "genv": "dist/index.cjs"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "env",
    "environment",
    "variables",
    "monorepo",
    "yaml",
    "config",
    "dotenv",
    "env-manager"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jirayusueb/genv.git"
  },
  "author": "jirayusueb",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "bun --hot src/index.ts",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "neverthrow": "^8.2.0",
    "yaml": "^2.8.1",
    "yargs": "^18.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@types/bun": "latest",
    "@types/yargs": "^17.0.34",
    "@vitest/coverage-v8": "^2.1.8",
    "tsdown": "^0.16.0",
    "ultracite": "6.3.0",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
