{
  "name": "genv",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.mts"
    }
  },
  "bin": {
    "genv": "./dist/index.mjs",
    "env": "./dist/index.mjs"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "bun --hot src/index.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "neverthrow": "^8.2.0",
    "yaml": "^2.8.1",
    "yargs": "^18.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@types/bun": "latest",
    "@types/yargs": "^17.0.34",
    "tsdown": "^0.16.0",
    "ultracite": "6.3.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
